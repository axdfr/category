<?xml version="1.0" encoding="ISO-8859-1"?>
<xesextension uri="https://github.com/axdfr/category/blob/master/CategoriesExtension.xml" prefix="cat" name="Categorization">
	<!-- Allows activities to belong to definable categories, that can be used in order to customize visualizations. -->
		<event>
			<list="Categories">
				<!-- Storing all categories that the respective event belongs to. -->
				<key="Category" value="name"/>
			</list>
		</event>
</xesextension>
